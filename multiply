// https://www.learncpp.com/cpp-tutorial/developing-your-first-program/

#include <iostream>

using namespace std;

int main()
{
    std::cout << "Enter an integer: " ;
    int x;
    std::cin >> x;
    std::cout << "Double " << x << " is " << 2*x << "\n";
    std::cout << "Triple " << x << " is " << 3*x;

    return 0;
}
